{
    "configurations": [
        {
            "name": "go test",
            "type": "go-test",
            "environment": {
                "GOPATH": "$USER_HOME$/.gvm/pkgsets/go1.18.10/global",
                "GOROOT": "$USER_HOME$/.gvm/gos/go1.18.10"
            },
            "buildParams": [
                "github.com/einouqo/castix",
                "--race"
            ],
            "goExecPath": "$USER_HOME$/.gvm/gos/go1.18.10/bin/go",
            "workingDir": "$PROJECT_DIR$",
            "allowParallelRun": true,
            "testRunParams": "-test.count=100",
        },
    ]
}